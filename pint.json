{
    "preset": "laravel",
    "rules": {
        "class_attributes_separation": {
            "elements": {
                "const": "none"
            }
        },
        "nullable_type_declaration": false,
        "nullable_type_declaration_for_default_null_value": false,
        "yoda_style": false,
        "unary_operator_spaces": false,
        "phpdoc_align": false,
        "not_operator_with_successor_space": false
    }
}
